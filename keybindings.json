// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+enter",
        "command": "jupyter.execCurrentCell"
    },
    {
        "key": "ctrl+shift+a",
        "command": "type",
        "args": { "text": "#%%" },
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+enter",
        "command": "workbench.action.terminal.runSelectedText"
    }
]